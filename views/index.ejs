<%- include('partials/header') -%>
    <header id="login">
    <nav id="adminNav">
    <span class="adminNavItem" id="adminLogin">admin</span>
    <a class="adminNavItem" href="/login">Login</a>
    <a class="adminNavItem" href="/signup">Signup</a>
    </nav>
    </header>
    <div class="container">
        <div class="d-flex justify-content-around flex-wrap">
        <% for(item of items) {%>
            <ul class="col-4 list-unstyled">
          <li class="mt-5 d-flex justify-content-center">
            <a href="/items/<%= items._id%>">
              <img class="" src="<%= item.image%>">
            </a>
          </li>
          <li class="d-flex justify-content-center">Item# <%= item.itemNum%></li>
          <li class="d-flex justify-content-center"><%= item.description%></li>
          <li class="d-flex justify-content-center">$<%= item.price%></li>
          <li class="d-flex justify-content-center"><button class="btn btn-primary"type="button">Add to Cart</button></li>
            </ul>
        <% } %>
    
    </div>
    </div>
    <div class="d-flex justify-content-center mt-5">
    <button class=" d-flex justify-content-center btn btn-dark">go to cart</button>
    </div>
        <%- include('partials/footer') -%>